        {
            "name": "debug_main_offline",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "--rescoring_weight",
                "1.0",
                "--ctc_weight",
                "0.0",
                "--reverse_weight",
                "0.0",
                "--chunk_size",
                "16",
                "--wav_path",
                "/root/test-16k.wav",
                "--model_path",
                "/root/kli/wenetmodel/ch1/final.zip",
                "--dict_path",
                "/root/kli/wenetmodel/ch1/units.txt",
